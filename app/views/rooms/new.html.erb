<% provide(:title, ' Create Room') %>
<h1>Create room</h1>
<p>ルームを作成しましょう</p>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
    <%= form_for(@room) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= f.label :room_name %>
        <%= f.text_field :name %>

        <%= f.submit "Create myroom", class: "btn btn-primary" %>
    <% end %>
    </div>
</div>